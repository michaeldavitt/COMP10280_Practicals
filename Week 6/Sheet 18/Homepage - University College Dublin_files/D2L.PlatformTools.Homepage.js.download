D2L.LP.Web.UI.Html.Dom.AddClassInitializer('d2l-homepage',function(panel){var cleanWidgetFrames=function(iframe){try{var doc=iframe.contentWindow.document;doc.body.style.backgroundColor='#fff';var links=doc.getElementsByTagName('a');for(var j=0;j<links.length;j++){var hasHref=D2L.LP.Web.UI.Html.Dom.HasAttribute(links[j],'href');if(!hasHref){continue;}if(!D2L.LP.Web.UI.Html.Dom.HasAttribute(links[j],'target')){links[j].target='_parent';}links[j].href=links[j].href.replace('&d2l_body_type=3','');links[j].href=links[j].href.replace('?d2l_body_type=3&','?');links[j].href=links[j].href.replace('?d2l_body_type=3','');}var forms=doc.getElementsByTagName('form');for(var k=0;k<forms.length;k++){if(!D2L.LP.Web.UI.Html.Dom.HasAttribute(forms[k],'target')){forms[k].target='_top';}}}catch(e){}};var legacyWidgetWrappers=D2L.LP.Web.UI.Html.Dom.GetElementsByClassName('d2l-widget-iframe-wrapper',panel);for(var i=0;i<legacyWidgetWrappers.length;i++){(function(legacyWidgetWrapper){var iframe=D2L.LP.Web.UI.Html.Dom.GetFirstElementByClassName('d2l-iframe',legacyWidgetWrapper);if(iframe===undefined||iframe===null){return;}D2L.LP.Web.UI.Desktop.Controls.IFrame.AddOnLoadHandler(iframe,function(){cleanWidgetFrames(iframe);});})(legacyWidgetWrappers[i]);}var cancelClickEvent=function(evt){D2L.LP.Web.UI.Html.Dom.CancelEventBubble(evt);};var headingLinks=D2L.LP.Web.UI.Html.Dom.GetElementsByClassName('d2l-homepage-heading-link',panel);for(var j=0;j<headingLinks.length;j++){D2L.LP.Web.UI.Html.Dom.AddEventListener('click',headingLinks[j],cancelClickEvent);}});D2L.LP.Web.UI.Html.Dom.AddClassInitializer('d2l-image-banner-overlay',function(node){node.courseImageUploadCb=D2L.LP.Web.UI.ObjectRepository.TryGet('courseImageUploadCb');node.addEventListener('d2l-image-banner-overlay-toggle-banner',function(e){var handler=D2L.LP.Web.UI.ObjectRepository.TryGet('imageBannerMenuItemToggleBannerHandler');if(handler){handler.call(handler);}});node.addEventListener('d2l-image-banner-overlay-change-banner-title',function(e){var handler=D2L.LP.Web.UI.ObjectRepository.TryGet('imageBannerMenuItemChangeBannerTitleHandler');if(handler){handler.call(handler);}});});D2L.LP.Web.UI.Html.Dom.AddClassInitializer('d2l-my-courses-widget',function(node){node.courseImageUploadCb=D2L.LP.Web.UI.ObjectRepository.TryGet('myCoursesImageUploadCb');});D2L=D2L||{};D2L.LP=D2L.LP||{};D2L.LP.Homepage={};D2L.LP.Homepage.Banner={ImageUploadSuccess:function(){var bannerOverlay=document.querySelector('d2l-image-banner-overlay');if(bannerOverlay){bannerOverlay.courseImageUploadCompleted(true);}}};D2L.LP.Homepage.Widget={GetWidgetElementByContentId:function(id){var content=document.getElementById(id.GetValue());if(!content){return;}return D2L.LP.Web.UI.Html.Dom.GetParentElementByClassName('d2l-widget',content);},ExpandWidgetContent:function(id){var widget=D2L.LP.Homepage.Widget.GetWidgetElementByContentId(id);if(!widget){return;}document.getElementById(id.GetValue()).expanded=true;widget.classList.remove('d2l-widget-collapsed');},CollapseWidgetContent:function(id){var widget=D2L.LP.Homepage.Widget.GetWidgetElementByContentId(id);if(!widget){return;}document.getElementById(id.GetValue()).expanded=false;widget.classList.add('d2l-widget-collapsed');}};D2L.LP.Homepage.MyCoursesWidget={ImageUploadSuccess:function(){var myCourses=document.querySelector('d2l-my-courses');if(myCourses){myCourses.courseImageUploadCompleted(true);}},GetLastOrgUnitId:function(){var myCourses=document.querySelector('d2l-my-courses');return myCourses?myCourses.getLastOrgUnitId():null;}};